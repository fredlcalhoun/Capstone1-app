<h1>Edit image</h1>
<%= form_tag "/images/#{@images.id}", method: "patch" do %>
  <div class="form-group">
    URL: <input type="text" class="form-control" value="<%= @image.url %>" name="">
  </div>
  <div class="form-group">
    Image id: <input type="text" class="form-control" value="<%= @image.image_id %>" name="image_id">
  </div>
  <input type="submit" class="btn btn-default" value="Edit pet">
<% end %>